{
  "name": "payment-bot",
  "description": "Bot da SeventyHost, só que agora de código aberto e feito em TypeScript.",
  "version": "2.0.0",
  "main": "dist/app.js",
  "scripts": {
    "start": "node .",
    "start:build": "npm run build && node .",
    "nodemon": "nodemon",
    "devs": "ts-node-dev --respawn --transpile-only --transpiler sucrase/ts-node-plugin -r tsconfig-paths/register src/app.ts",
    "dev": "ts-node -r tsconfig-paths/register --transpiler sucrase/ts-node-plugin src/app.ts",
    "build": "tsc && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "build:test": "tsc --project ./tsconfig.json --declaration true -declarationDir dist/types && tsc-alias",
    "lint": "eslint --ext .tsx,.ts --fix ./src -c .eslintrc.json",
    "compress": "npm run build:test && ts-node compress.ts",
    "rollup": "npm run build:test && rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript --bundleConfigAsCjs"
  },
  "keywords": [
    "Bot",
    "Payment",
    "Discord",
    "Mercado Pago",
    "MP"
  ],
  "author": "Ashu11-a",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ashu11-A/PaymentBot.git"
  },
  "bugs": {
    "url": "https://github.com/Ashu11-A/PaymentBot/issues"
  },
  "homepage": "https://github.com/Ashu11-A/PaymentBot#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-eslint": "^9.0.5",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-multi-entry": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.18",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/mercadopago": "^1.5.9",
    "@types/node-telegram-bot-api": "^0.64.0",
    "@types/randomstring": "^1.1.8",
    "@types/signale": "^1.4.4",
    "@types/uglify-js": "^3.17.4",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "eslint": "^8.47.0",
    "eslint-config-standard-with-typescript": "^38.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.0.2",
    "eslint-plugin-promise": "^6.1.1",
    "rollup-plugin-rename": "^1.0.1",
    "rollup-plugin-tsconfig-paths": "^1.5.2",
    "rollup-plugin-typescript-paths": "^1.5.0",
    "sucrase": "^3.34.0",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "tscpaths": "^0.0.9",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@magicyan/discord": "^1.0.3",
    "@types/inquirer": "^9.0.6",
    "@types/node": "^20.6.2",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/parser": "^6.4.1",
    "axios": "^1.5.0",
    "bcrypt": "^5.1.1",
    "better-sqlite3": "^8.6.0",
    "body-parser": "^1.20.2",
    "chalk": "^4.1.2",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
