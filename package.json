    "eslint": "^8.46.0",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "glob": "^10.3.3",
    "helmet": "^7.0.0",
    "inquirer": "^9.2.11",
    "jsonwebtoken": "^9.0.2",
    "mercadopago": "^2.0.3",
    "mongoose": "^8.1.1",
    "mysql2": "^3.9.1",
    "node-telegram-bot-api": "^0.63.0",
    "quick.db": "^9.1.7",
    "randomstring": "^1.3.0",
    "rollup": "^4.9.6",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "set-interval-async": "^3.0.3",
    "signale": "^1.4.0",
    "telegraf": "^4.14.0",
    "terser": "^5.27.0",
    "ts-lib": "^0.0.5",
